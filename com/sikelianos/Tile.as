package com.sikelianos {	import flash.display.Sprite	import flash.events.*	public class Tile extends Sprite {	  var _x_offset	  var _y_offset	  		function Tile(x_offset:Number=0, y_offset:Number=0) {      _x_offset = x_offset      _y_offset = y_offset      addEventListener(Event.ADDED_TO_STAGE, init)    }        function init(e:Event) {      x = width * _x_offset      y = height * _y_offset    }    	}	}